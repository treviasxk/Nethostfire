<Project Sdk="Microsoft.NET.Sdk">
  <ItemGroup>
    <Reference Include="Unity/UnityEngine.dll">
      <SpecificVersion>False</SpecificVersion>
    </Reference>
  </ItemGroup>
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Version>2.0.5.0</Version>
    <AssemblyVersion>2.0.5.0</AssemblyVersion>
    <FileVersion>2.0.5.0</FileVersion>
    <Company>Trevias Xk</Company>
    <Authors>Trevias Xk</Authors>
    <Product>Nethostfire</Product>
    <Description>Nethostfire is a server created in Netstandard2.0 with C#, it's a simple and easy-to-understand project, it's perfect for creating game servers like Unity 3D for example.</Description>
    <Copyright>Trevias Xk Â© 2022</Copyright>
    <RootNamespace>Nethostfire</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <InvariantGlobalization>false</InvariantGlobalization>
  </PropertyGroup>
</Project>